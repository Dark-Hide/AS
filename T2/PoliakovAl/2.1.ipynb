    "def prostoe(x):\n",
    "    for i in range(2,x+1):\n",
    "        b = 0\n",
    "        for j in range(2,i):\n",
    "            if i%j==0:\n",
    "                b=1\n",
    "                break\n",
    "        if b==0:\n",
    "            print(i);\n",
    "a=int(input(\"Введите число:\"))\n",
    "prostoe(a)"
